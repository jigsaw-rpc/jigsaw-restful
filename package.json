{
  "name": "jigsaw-router",
  "version": "0.0.1-dev1",
  "description": "jigsaw-router is a router component of jigsaw-rpc",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "test": "mocha",
    "build": "tsc",
    "prepare": "npm run build",
    "test-cov": "nyc --reporter=lcov npm test",
