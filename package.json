{
  "name": "jigsaw-restful",
  "version": "0.0.6",
  "description": "jigsaw-restful is a router component of jigsaw-rpc",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "test": "mocha",
    "compile": "tsc",
    "build": "node ./scripts/build",
    "prepare": "npm run compile",
    "test-cov": "nyc --reporter=lcov npm test",
    "dev": "nodemon --watch ./src/ --watch ./examples/ -e ts --exec \"ts-node ./examples/test-app.ts\" ",
    "start": "node ./bin/cli",
    "deploy": "node ./scripts/deploy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jigsaw-rpc/jigsaw-restful.git"
  },
  "keywords": [
    "router",
    "jigsaw"
  ],
  "author": "zhymc",
  "license": "GPL-2.0",
  "bugs": {
    "url": "https://github.com/jigsaw-rpc/jigsaw-restful/issues"
  },
  "homepage": "https://github.com/jigsaw-rpc/jigsaw-restful#readme",
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.14.25",
    "cz-conventional-changelog": "^3.3.0",
    "debug": "^4.2.0",
    "jigsaw-net": "0.0.2-dev9",
    "mocha": "^8.3.0",
    "nyc": "^15.1.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.1.5"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    },
    "org": "wyattzhy",
    "expose_port": [
      "1900"
    ]
  },
  "dependencies": {
    "@types/koa": "^2.11.6",
    "@types/validatorjs": "^3.15.0",
    "config-style-cli": "^0.0.2",
    "debug": "^4.2.0",
    "jigsaw-rpc": "^0.1.2",
    "koa": "^2.13.1",
    "koa-bodyparser": "^4.3.0",
    "koa-compose": "^4.1.0",
    "koa-useragent": "^4.0.0",
    "meow": "^9.0.0",
    "route-parser": "0.0.5",
    "tiny-typed-emitter": "^2.0.3",
    "url-pattern": "^1.0.3",
    "validatorjs": "^3.21.0"
  }
}
